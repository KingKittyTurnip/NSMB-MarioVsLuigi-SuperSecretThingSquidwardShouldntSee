  - speakerName: Urio
    speakerColor: {r: 0, g: 1, b: 1, a: 1}
    dialogue: 'He seems awfully familiar. '
    speaker: 0
    speakerImage: {fileID: 990936703, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: 0}
    autoProgress: 0
  - speakerName: Urio
    speakerColor: {r: 0, g: 1, b: 1, a: 1}
    dialogue: 'That guy over there looks a lot like me, he''s probably the protagonist. '
    speaker: 0
    speakerImage: {fileID: 990936703, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: 307322972, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
