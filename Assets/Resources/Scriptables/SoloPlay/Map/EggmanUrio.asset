  - speakerName: Urio
    speakerColor: {r: 0, g: 1, b: 1, a: 1}
    dialogue: So you're the protagonist around here?
    speaker: 0
    speakerImage: {fileID: 990936703, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: -1868981158, guid: 933ec60777c4c574bb3ded23a7117649,
