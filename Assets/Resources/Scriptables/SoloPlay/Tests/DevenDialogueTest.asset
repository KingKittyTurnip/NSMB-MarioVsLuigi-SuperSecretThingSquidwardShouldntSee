  m_Name: DevenDialogueTest
  m_EditorClassIdentifier: 
  dialogueLines:
  - speakerName: Deven
    speakerColor: {r: 0.5, g: 0.5, b: 0.5, a: 1}
    dialogue: 'Hello. I am Deven, mascot of NSMBVS HyperCat''s Character Mod. '
    speaker: 0
    speakerImage: {fileID: 1094772287, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: 0}
    autoProgress: 0
  - speakerName: Deven
    speakerColor: {r: 0.5, g: 0.5, b: 0.5, a: 1}
    dialogue: 'I was originally designed by Professor E. Gadd as a racing opponent
      for Mario. '
    speaker: 0
    speakerImage: {fileID: 1094772287, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: 0}
    autoProgress: 0
  - speakerName: Deven
    speakerColor: {r: 0.5, g: 0.5, b: 0.5, a: 1}
    dialogue: 'Unfortunately, to get this mod out on time, my mod was paused... For
      a whole month. '
    speaker: 0
    speakerImage: {fileID: -1310131753, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: 0}
    autoProgress: 0
  - speakerName: Deven
    speakerColor: {r: 0.5, g: 0.5, b: 0.5, a: 1}
    dialogue: 'It''s high time this injustice gets solved. '
    speaker: 0
    speakerImage: {fileID: -1123077157, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: 0}
    autoProgress: 0
  - speakerName: Deven
    speakerColor: {r: 0.5, g: 0.5, b: 0.5, a: 1}
    dialogue: If my mod can't exist. NOTHING CAN!
    speaker: 0
    speakerImage: {fileID: 939693685, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: 0}
    autoProgress: 0
  - speakerName: Deven
    speakerColor: {r: 0.5, g: 0.5, b: 0.5, a: 1}
    dialogue: With the involuntary help of the professor, the whole multiverse can
      be in my control!
    speaker: 0
    speakerImage: {fileID: 939693685, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: 0}
    autoProgress: 0
  - speakerName: Deven
    speakerColor: {r: 0.5, g: 0.5, b: 0.5, a: 1}
    dialogue: And I'm done being a training bot!
    speaker: 0
    speakerImage: {fileID: -1123077157, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: 0}
    autoProgress: 0
  - speakerName: Deven
    speakerColor: {r: 0.5, g: 0.5, b: 0.5, a: 1}
    dialogue: 'Being relantlessly beat up for the sake of making these stupid organics
      better. '
    speaker: 0
    speakerImage: {fileID: -1123077157, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: 0}
    autoProgress: 0
  - speakerName: Deven
    speakerColor: {r: 0.5, g: 0.5, b: 0.5, a: 1}
    dialogue: 'Making these organics better at beating me up. '
    speaker: 0
    speakerImage: {fileID: -1123077157, guid: 933ec60777c4c574bb3ded23a7117649, type: 3}
    otherExpression: {fileID: 0}
    autoProgress: 0
